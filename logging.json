{
    "version": 1,
    "disable_existing_loggers": false,

    "formatters": {
      "simple": {
        "format": "%(levelname)s: %(name)s: %(message)s"
      }
    },

    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "simple",
        "stream": "ext://sys.stdout"
      }
    },

    "root": {
      "level": "INFO",
      "handlers": ["console"]
    }
}
